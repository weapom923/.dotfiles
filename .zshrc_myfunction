#==============================================================================
function reset_my_zsh() {
  source ~/.zshrc 
}

#==============================================================================
function open_web_with_query {
  local url=$1
  local query=$2

  open "${url}${query}"
}

#==============================================================================
function eijiro {
  open_web_with_query "http://eow.alc.co.jp/search?q=" $*
}
